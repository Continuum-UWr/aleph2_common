<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"
       name="aleph2">

  <xacro:include filename="$(find aleph2_description)/urdf/aleph2.gazebo" />
  <xacro:include filename="$(find aleph2_description)/urdf/materials.xacro" />

  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://aleph2_description/meshes/base.STL" />
      </geometry>
      <origin xyz="0 0 0" rpy="${pi/2} 0 ${pi/2}"/>
      <material name="base" />
    </visual>
    <visual>
      <geometry>
        <cylinder radius="0.02" length="0.12" />
      </geometry>
      <origin xyz="0.06 0.125 0.063" />
      <material name="motor">
        <color rgba="0 0 0.5 1"/>
      </material>
    </visual>
    <visual>
      <geometry>
        <cylinder radius="0.016" length="0.1" />
      </geometry>
      <origin xyz="0.151 0.175 0.053" />
      <material name="encoder">
        <color rgba="0 0.5 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.72 0.5 0.1"/>
      </geometry>
      <origin xyz="-0.08 0 0"/>
    </collision>
    <collision>
      <geometry>
        <box size="0.16 0.5 0.1"/>
      </geometry>
      <origin xyz="-0.06 0 0.09"/>
    </collision>
    <collision>
      <geometry>
        <cylinder radius="0.02" length="0.12" />
      </geometry>
      <origin xyz="0.06 0.125 0.063" />
    </collision>
    <collision>
      <geometry>
        <cylinder radius="0.016" length="0.1" />
      </geometry>
      <origin xyz="0.151 0.175 0.053" />
    </collision>
    <inertial>
      <origin xyz="-0.06700 0.00064 0.00551"/>
      <mass value="9.0" />
      <inertia ixx="0.23691" ixy="-0.00014" ixz="-0.00314" 
               iyy="0.23328" iyz="-0.00054" izz="0.40961" />
    </inertial>
  </link>

  <xacro:macro name="rocker" params="name">
    <link name="rocker_${name}_link">
      <visual>
        <geometry>
          <mesh filename="package://aleph2_description/meshes/rocker.STL"/>
        </geometry>
        <material name="rocker" />
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://aleph2_description/meshes/rocker_collision.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0.00000 -0.01737 0.03763"/>
        <mass value="1.5" />
        <inertia ixx="0.01934" ixy="-0.00942" ixz="-0.00271" 
                 iyy="0.03716" iyz="0.00516"  izz="0.04409" />
      </inertial>
    </link>
  </xacro:macro>

  <xacro:macro name="wheel" params="name"> 
    <link name="wheel_${name}_link">
      <visual>
        <geometry>
          <mesh filename="package://aleph2_description/meshes/wheel.STL"/>
        </geometry>
        <material name="wheel" />
      </visual>
      <visual>
        <geometry>
          <cylinder radius="0.05" length="0.12"/>
        </geometry>
        <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
        <material name="piasta">
          <color rgba="0.5 0.5 0.5 1" />
        </material>
      </visual>
      <visual>
        <geometry>
          <cylinder radius="0.0055" length="0.15"/>
        </geometry>
        <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
        <material name="walek">
          <color rgba="0.7 0.7 0.7 1" />
        </material>
      </visual>
      <collision>
        <geometry>
          <cylinder radius="0.18" length="0.1" />
        </geometry>
        <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
      </collision>
      <collision>
        <geometry>
          <cylinder radius="0.007" length="0.15"/>
        </geometry>
        <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
      </collision>
      <inertial>
        <origin xyz="0.00000 0.00000 0.00000"/>
        <mass value="6.0" />
        <inertia ixx="0.03845" ixy="0.00000" ixz="0.00000" 
                 iyy="0.06495" iyz="0.00000" izz="0.03845" />
      </inertial>
    </link>
  </xacro:macro>

  <xacro:rocker name="R" />
  <xacro:rocker name="L" />

  <xacro:wheel name="FR" />
  <xacro:wheel name="RR" />
  <xacro:wheel name="FL" />
  <xacro:wheel name="RL" />

  <link name="manip_base_link">
    <visual>
      <geometry>
          <mesh filename="package://aleph2_description/meshes/manip_base.STL"/>
      </geometry>
      <origin rpy="${pi/2} 0 ${-pi/2}"/>
      <material name="manip" />
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.125" length="0.02" />
      </geometry>
      <origin xyz="0 0 0.01"/>
    </collision>
    <collision>
      <geometry>
        <cylinder radius="0.11" length="0.025" />
      </geometry>
      <origin xyz="0 0.017 0.09" rpy="${pi/2} 0 0"/>
    </collision>
    <collision>
      <geometry>
        <box size="0.1 0.15 0.15"/>
      </geometry>
      <origin xyz="0 0 0.075"/>
    </collision>
    <collision>
      <geometry>
        <box size="0.1 0.22 0.10"/>
      </geometry>
      <origin xyz="0 0 0.05"/>
    </collision>
    <inertial>
      <origin xyz="-0.00251 0.00769 0.05483"/>
      <mass value="2.0" />
      <inertia ixx="0.00845" ixy="-0.00007" ixz="-0.00023" 
               iyy="0.00892" iyz="0.00049"  izz="0.00862" />
    </inertial>
  </link>

  <link name="manip_shoulder_link">
    <visual>
      <geometry>
          <mesh filename="package://aleph2_description/meshes/manip_shoulder.STL"/>
      </geometry>
      <origin rpy="${pi} 0 0"/>
