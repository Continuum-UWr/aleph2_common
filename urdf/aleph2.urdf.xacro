  <!-- CHROMOSOMY -->

  <joint name="chromosom_R_to_base" type="fixed">
    <parent link="base_link"/>
    <child link="chromosom_R"/>
      <origin xyz="0 -0.375 0" rpy="${pi/2} 0 ${pi}"/>
  </joint>

  <joint name="chromosom_L_to_base" type="fixed">
    <parent link="base_link"/>
    <child link="chromosom_L"/>
      <origin xyz="0 0.375 0" rpy="${pi/2} 0 0"/>
  </joint>

  <!-- KOLA -->

  <joint name="kolo_FR_to_chromosom_R" type="continuous">
    <parent link="chromosom_R"/>
    <child link="kolo_FR"/>
    <origin xyz="-0.418 -0.226 -0.07" rpy="${pi/2} 0 0" />
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="kolo_RR_to_chromosom_R" type="continuous">
    <parent link="chromosom_R"/>
    <child link="kolo_RR"/>
    <origin xyz="0.418 -0.226 -0.07" rpy="${-pi/2} 0 0" />
    <axis xyz="0 -1 0"/>
  </joint>

  <joint name="kolo_FL_to_chromosom_L" type="continuous">
    <parent link="chromosom_L"/>
    <child link="kolo_FL"/>
    <origin xyz="0.418 -0.226 -0.07" rpy="${-pi/2} 0 0" />
    <axis xyz="0 1 0"/>
  </joint>

  <joint name="kolo_RL_to_chromosom_L" type="continuous">
    <parent link="chromosom_L"/>
    <child link="kolo_RL"/>
    <origin xyz="-0.418 -0.226 -0.07" rpy="${pi/2} 0 0" />
    <axis xyz="0 -1 0"/>
  </joint>

  <!-- MANIP -->

  <!-- <joint name="support_to_base" type="fixed">
    <parent link="base_link"/>
    <child link="manip_support"/>
    <origin xyz="0.151 0.0035 0.0"/>
  </joint> -->

  <joint name="base_to_frame" type="continuous">
    <parent link="base_link"/>
    <child link="manip_base"/>
    <origin xyz="0.151 0.0035 0.032"/>
    <axis xyz="0 0 1"/>
    <limit effort="32000" velocity="0.15"/> 
    <dynamics damping="10" friction="0.1"/>
  </joint>

  <joint name="B_to_base" type="revolute">
    <parent link="manip_base"/>
    <child link="manip_B"/>
    <origin xyz="0 0 0.09" rpy="${pi/2} 0 0"/>
    <axis xyz="0 0 1" />
    <limit effort="32000" lower="0.0" upper="3.14" velocity="0.3"/>
    <dynamics damping="10" friction="0.1"/>
  </joint>

  <joint name="C_to_B" type="revolute">
    <parent link="manip_B"/>
    <child link="manip_C"/>
    <origin xyz="0.4 0 0" rpy="${pi} 0 0" />
    <axis xyz="0 0 -1" />
    <limit effort="32000" lower="-2.28" upper="2.43" velocity="0.5"/>
    <dynamics damping="10" friction="0.1"/>
  </joint>

  <joint name="efector_to_C" type="revolute">
    <parent link="manip_C"/>
    <child link="manip_D"/>
    <origin xyz="0.4 0 0" />
    <axis xyz="0 0 -1" />
    <limit effort="32000" lower="-2.2" upper="2.2" velocity="0.5"/>
    <dynamics damping="10" friction="0.1"/>
  </joint>

  <joint name="efector_roll" type="revolute">
    <parent link="manip_D"/>
    <child link="manip_efector"/>
    <origin xyz="0.06 0 0" rpy="${-pi/2} 0 0"/>
    <axis xyz="-1 0 0" />
    <limit effort="32000" lower="${-pi}" upper="${pi}" velocity="0.5"/>
    <dynamics damping="10" friction="0.1"/>
  </joint>

  <joint name="efector_grip" type="prismatic">
    <parent link="manip_efector"/>
    <child link="manip_jaw_L"/>
    <origin xyz="0.025 -0.03 0"/>
    <axis xyz="0 1 0" />
    <limit effort="150" lower="-0.033" upper="0" velocity="0.05"/>
    <dynamics damping="10" friction="0.1"/>
  </joint>

  <joint name="efector_grip_mimic" type="prismatic">
    <parent link="manip_efector"/>
    <child link="manip_jaw_R"/>
    <origin xyz="0.025 0.03 0"/>
    <axis xyz="0 -1 0" />
    <mimic joint="efector_grip" />
    <limit effort="150" lower="-0.033" upper="0" velocity="0.05"/>
    <dynamics damping="10" friction="0.1"/>
  </joint>

  <joint name="efector_tip_to_manip_efector" type="fixed">
    <parent link="manip_efector"/>
    <child link="efector_tip"/>
    <origin xyz="0.20 0 0"/>
    <dynamics damping="10" friction="0.1"/>
  </joint>

  <!-- MASZT -->

  <joint name="maszt_to_base" type="fixed">
    <parent link="base_link" />
    <child link="maszt" />
    <origin xyz="-0.22 0.25 -0.15"/>
  </joint>

  <!-- ZED -->

  <joint name="zed_camera_to_base" type="fixed">
    <parent link="base_link"/>
    <child link="zed_position"/>
    <origin xyz="0.265 -0.16 0.015"/>
  </joint>

    
  <transmission name="kolo_FR_transmission">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="kolo_FR_to_chromosom_R">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="kolo_FR_actuator">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="kolo_FL_transmission">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="kolo_FL_to_chromosom_L">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="kolo_FL_actuator">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="kolo_RR_transmission">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="kolo_RR_to_chromosom_R">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="kolo_RR_actuator">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="kolo_RL_transmission">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="kolo_RL_to_chromosom_L">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="kolo_RL_actuator">
