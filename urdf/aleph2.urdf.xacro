<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"
       name="aleph2">


  <xacro:include filename="$(find aleph2_description)/urdf/aleph2.gazebo" />

  <material name="lazik">
    <color rgba="0.3 0.3 0.3 1"/>
  </material>
  <material name="chromosom">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="kolo">
    <texture filename="package://aleph_viz/textures/wheel.jpg" />
  </material>
  <material name="manip">
    <color rgba="0.4 0.4 0.4 1"/>
  </material>
  <material name="maszt">
    <color rgba="0.8 0.8 0.8 1" />
  </material>

  <xacro:macro name="chromosom" params="name">
    <link name="chromosom_${name}">
      <visual>
        <geometry>
          <mesh filename="package://aleph_viz/meshes/chromosom.STL"/>
        </geometry>
        <material name="chromosom" />
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://aleph_viz/meshes/chromosom_collision.STL" />
        </geometry>
      </collision>
      <inertial>
        <origin xyz="0.00000 -0.01737 0.03763"/>
        <mass value="1.5" />
        <inertia ixx="0.01934" ixy="-0.00942" ixz="-0.00271" 
                 iyy="0.03716" iyz="0.00516"  izz="0.04409" />
      </inertial>
    </link>
  </xacro:macro>

  <xacro:macro name="kolo" params="name"> 
    <link name="kolo_${name}">
      <visual>
        <geometry>
          <mesh filename="package://aleph_viz/meshes/kolo.STL"/>
        </geometry>
        <material name="kolo" />
      </visual>
      <visual>
        <geometry>
          <cylinder radius="0.05" length="0.12"/>
        </geometry>
        <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
        <material name="piasta">
          <color rgba="0.5 0.5 0.5 1" />
        </material>
      </visual>
      <visual>
        <geometry>
          <cylinder radius="0.0055" length="0.15"/>
        </geometry>
        <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
        <material name="walek">
          <color rgba="0.7 0.7 0.7 1" />
        </material>
      </visual>
      <collision>
        <geometry>
          <cylinder radius="0.18" length="0.1" />
        </geometry>
        <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
      </collision>
      <collision>
        <geometry>
          <cylinder radius="0.007" length="0.15"/>
        </geometry>
        <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
      </collision>
      <inertial>
        <origin xyz="0.00000 0.00000 0.00000"/>
        <mass value="6.0" />
        <inertia ixx="0.03845" ixy="0.00000" ixz="0.00000" 
                 iyy="0.06495" iyz="0.00000" izz="0.03845" />
      </inertial>
    </link>
  </xacro:macro>

